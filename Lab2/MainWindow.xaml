<!-- MainWindow.xaml -->
<Window x:Class="Lab2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Lab2.Views"
        Title="Lab2" MaxHeight="1000" MaxWidth="1000">
    <TabControl>
        <TabItem Header="File Explorer">
            <local:FileExplorerView/>
        </TabItem>
        <TabItem Header="Help">
            <local:HelpView/>
        </TabItem>
        <TabItem Header="Command Line">
            <local:CommandLineView/>
        </TabItem>
    </TabControl>
</Window>
